{
  "bail": true,
  "testEnvironment": "node",
  "collectCoverageFrom": [
    "/src/*js"
  ],
  "coveragePathIgnorePatterns": [
    "./webpack.dev.config.js",
    "./webpack.prod.config.js",
    "/coverage/",
    "/dist/",
    "/node_modules/"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 70,
      "functions": 70,
      "lines": 70,
      "statements": 70
    }
  }
}
