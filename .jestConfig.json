{
  "bail": true,
  "testEnvironment": "node",
  "collectCoverageFrom": [
    "src/modules/weather/*.js"
  ],
  "coveragePathIgnorePatterns": [
    "./webpack.dev.config.js",
    "./webpack.prod.config.js",
    "./coverage/",
    "./dist/"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 70,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  }
}
