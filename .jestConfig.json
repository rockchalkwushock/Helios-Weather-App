{
  "bail": true,
  "testEnvironment": "node",
  "collectCoverageFrom": [
    "src/modules/weather/*.js"
  ],
  "coveragePathIgnorePatterns": [
    "src/modules/weather/actions.js"
  ],
  "coverageThreshold": {
    "global": {
