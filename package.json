{
  "name": "helios-weather-app",
  "version": "0.1.0",
  "description": "Weather App using OpenWeatherMap Api.",
  "main": "index.js",
  "repository": "https://github.com/rockchalkwushock/Helios-Weather-App.git",
  "author": "Cody <rockchalkwushock@iclould.com> (https://rockchalkwushock.github.io/rcws-development/)",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && NODE_ENV=production webpack -p --progress --profile --colors --config webpack.prod.config.js",
    "clean": "rimraf dist coverage",
    "commit": "git cz",
    "dev:c": "NODE_ENV=development webpack-dev-server --colors --profile --config webpack.dev.config.js",
    "dev:s": "NODE_ENV=development nodemon index.js",
    "lint": "eslint src api",
    "precommit": "yarn lint && yarn test",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "start": "NODE_ENV=production nodemon index.js",
    "test": "jest --config=.jestConfig.json",
    "test:coverage": "yarn test -- --coverage",
    "test:watch": "yarn test -- --watch"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "codecov": "^2.1.0",
    "commitizen": "^2.9.6",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-equimper": "^1.6.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "image-webpack-loader": "^3.3.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "nodemon": "^1.11.0",
    "offline-plugin": "^4.6.2",
    "postcss-loader": "^1.3.3",
    "react-hot-loader": "3.0.0-beta.6",
    "redbox-react": "^1.3.4",
    "redux-devtools-extension": "^2.13.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "sw-precache-webpack-plugin": "^0.9.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^0.16.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "normalize.css": "^6.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "3.0.3",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^1.4.4"
  }
}
