{
  "name": "helios-weather-app",
  "version": "2.0.0-alpha.0",
  "description": "Progressive Web Application built with React & Redux for weather monitory using the Weatherbit.io API.",
  "main": "index.js",
  "repository": "https://github.com/rockchalkwushock/Helios-Weather-App.git",
  "author": "Cody <rockchalkwushock@iclould.com> (https://rockchalkwushock.github.io/rcws-development/)",
  "license": "MIT",
  "scripts": {
    "build": "",
    "clean:repo": "rimraf coverage/ dist/",
    "commit": "git cz",
    "dev:c": "",
    "dev:s": "NODE_ENV=development nodemon index.js",
    "lint": "eslint api/ src/",
    "precommit": "yarn lint && yarn test",
    "push": "git push origin v2.0.0",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "start": "NODE_ENV=production nodemon index.js",
    "test": "BABEL_ENV=test jest --config=.jestConfig.json",
    "test:coverage": "yarn test -- --coverage",
    "test:watch": "yarn test -- --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "codecov": "^2.1.0",
    "cz-conventional-changelog": "2.0.0",
    "dotenv-safe": "^4.0.3",
    "eslint": "^3.19.0",
    "eslint-config-equimper": "^1.6.2",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "nodemon": "^1.11.0",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^0.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.2.0",
    "koa-compose": "^3.2.1",
    "koa-helmet": "^3.1.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
