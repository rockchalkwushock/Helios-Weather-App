  "license": "MIT",
  "scripts": {
    "build": "",
    "clean:repo": "rimraf coverage/ dist/",
    "commit": "git cz",
    "dev:c": "BABEL_ENV=development NODE_ENV=development webpack-dev-server --colors --profile --config webpack.dev.config.js",
    "dev:s": "NODE_ENV=development nodemon index.js",
    "lint": "eslint api/ src/",
    "precommit": "yarn lint && yarn test",
    "push": "git push origin v2.0.0",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "start": "NODE_ENV=production nodemon index.js",
    "test": "BABEL_ENV=test jest --config=.jestConfig.json",
    "test:coverage": "yarn test -- --coverage",
    "test:watch": "yarn test -- --watch"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
